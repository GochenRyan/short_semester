package com.posts.pojo;

public class PostTransfer {
	private int empno;
    private int OldPno;
    private int newPno;
    private String postTransferType;
    private String postTransferCause;
    private String postTransferRemark;
	public int getEmpno() {
		return empno;
	}
	public void setEmpno(int empno) {
		this.empno = empno;
	}
	public int getOldPno() {
		return OldPno;
	}
	public void setOldPno(int oldPno) {
		OldPno = oldPno;
	}
	public int getNewPno() {
		return newPno;
	}
	public void setNewPno(int newPno) {
		this.newPno = newPno;
	}
	public String getPostTransferType() {
		return postTransferType;
	}
	public void setPostTransferType(String postTransferType) {
		this.postTransferType = postTransferType;
	}
	public String getPostTransferCause() {
		return postTransferCause;
	}
	public void setPostTransferCause(String postTransferCause) {
		this.postTransferCause = postTransferCause;
	}
	public String getPostTransferRemark() {
		return postTransferRemark;
	}
	public void setPostTransferRemark(String postTransferRemark) {
		this.postTransferRemark = postTransferRemark;
	}
	
}
