package com.posts.pojo;

public class PostEmp {
	private int pno;
	private String pname;
	private int  empno;
	private String ename;
	private String esex;
	private String if_pro; //是否试用中
	private String if_train; //是否培训中
	private String if_dim; //是否离职 
	public int getPno() {
		return pno;
	}
	public void setPno(int pno) {
		this.pno = pno;
	}
	public String getPname() {
		return pname;
	}
	public void setPname(String pname) {
		this.pname = pname;
	}
	public int getEmpno() {
		return empno;
	}
	public void setEmpno(int empno) {
		this.empno = empno;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public String getEsex() {
		return esex;
	}
	public void setEsex(String esex) {
		this.esex = esex;
	}
	public String getIf_pro() {
		return if_pro;
	}
	public void setIf_pro(String if_pro) {
		this.if_pro = if_pro;
	}
	public String getIf_train() {
		return if_train;
	}
	public void setIf_train(String if_train) {
		this.if_train = if_train;
	}
	public String getIf_dim() {
		return if_dim;
	}
	public void setIf_dim(String if_dim) {
		this.if_dim = if_dim;
	}
	

}
